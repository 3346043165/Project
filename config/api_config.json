{
  "base_url": "https://api.example.com",
  "apis": {
    "login": {
      "path": "/auth/login",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    "get_user": {
      "path": "/users/{user_id}",
      "method": "GET",
      "headers": {
        "Authorization": "Bearer {token}"
      }
    },
    "create_hotel_order": {
      "path": "/hotels/orders",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer {token}"
      }
    }
  },
  "test_data": {
